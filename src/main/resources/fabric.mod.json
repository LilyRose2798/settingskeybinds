{
  "schemaVersion": 1,
  "id": "settingskeybinds",
  "version": "${version}",
  "name": "Settings Keybinds",
  "description": "Adds key bindings to change Minecraft settings.",
  "authors": [
    "LilyRose2798"
  ],
  "contact": {
    "homepage": "https://github.com/LilyRose2798/settingskeybinds",
    "sources": "https://github.com/LilyRose2798/settingskeybinds",
    "issues": "https://github.com/LilyRose2798/settingskeybinds/issues"
  },
  "license": "MIT",
  "icon": "assets/settingskeybinds/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "lgbt.lily.settingskeybinds.SettingsKeybinds"
    ],
    "modmenu": [
      "lgbt.lily.settingskeybinds.config.SettingsKeybindsModMenu"
    ]
  },
  "mixins": [
    "settingskeybinds.mixins.json"
  ],
  "depends": {
    "fabric-key-binding-api-v1": "*",
    "fabricloader": ">=0.11.3",
